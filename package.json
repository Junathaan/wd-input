{
  "name": "wd-input",
  "version": "0.0.0-development",
  "repository": "junathaan/wd-input",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name wd ./src/plugin.js",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "jest": "jest",
    "mocha": "mocha-webpack --webpack-config config/webpack.config.js --require ./test/setup.js ./test/*.test.js",
    "semantic-release": "semantic-release"
  },
  "files": [
    "src/*",
    "dist/*",
    "*.json",
    "*.js"
  ],
  "main": "./dist/wd.common.js",
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@semantic-release/changelog": "^3.0.2",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/vue": "^5.0.6",
    "@vue/babel-preset-app": "^3.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "expect": "^24.7.1",
    "jest": "^24.7.1",
    "jsdom": "14.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.1.3",
    "mocha-webpack": "^2.0.0-beta.0",
    "semantic-release": "^15.13.3",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branch": "travis",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "./CHANGELOG.md"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
